<refentry xml:id="arrayaccesstktv.offsetunset" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ArrayAccess&lt;Tk, Tv&gt;::offsetUnset</refname>
  <refpurpose>Offset to unset</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>abstract</modifier> <modifier>public</modifier> <type>this</type><methodname>ArrayAccess&lt;Tk, Tv&gt;::offsetUnset</methodname>
   <methodparam><type>Tk</type><parameter>key</parameter></methodparam>
  </methodsynopsis>
  <para>
   Unsets an offset.
  </para>
  <note>
   <para>
    This method will <emphasis>not</emphasis> be called when type-casting to
    <link linkend="language.types.typecasting">(unset)</link>
   </para>
  </note>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>key</parameter></term>
     <listitem>
      <para>
       The offset to unset.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>

 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   this
  </para>
 </refsect1>

</refentry>

