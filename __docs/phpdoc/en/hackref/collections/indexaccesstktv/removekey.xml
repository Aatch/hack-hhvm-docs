<refentry xml:id="hack.indexaccesstktv.removekey" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>IndexAccess&lt;Tk, Tv&gt;::removeKey</refname>
  <refpurpose>Remove the key (and associated value) from the collection</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>abstract</modifier> <modifier>public</modifier> <type>this</type><methodname>IndexAccess&lt;Tk, Tv&gt;::removeKey</methodname>
   <methodparam><type>Tk</type><parameter>k</parameter></methodparam>
  </methodsynopsis>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>k</parameter></term>
     <listitem>
      <para>
       The key to remove.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   The updated collection.
  </para>
 </refsect1>


</refentry>

