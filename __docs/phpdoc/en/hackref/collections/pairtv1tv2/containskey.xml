<refentry xml:id="hack.pairtv1tv2.containskey" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Pair&lt;Tv1, Tv2&gt;::containsKey</refname>
  <refpurpose>Check if the specified key is present in the Pair</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier>
   <type>bool</type>
   <methodname>Pair&lt;Tv1, Tv2&gt;::containsKey</methodname>
   <methodparam><type>int</type><parameter>k</parameter></methodparam>
  </methodsynopsis>

<!-- Note: If this method/property as an alternate procedural mapping then the method/property synopsis MUST be listed BEFORE the function synopsis -->
  <para>
    Check if the specified key is present in the Pair. Will always return true for <literal>$k</literal> 0 or 1; false otherwise.
  </para>
 </refsect1>
</refentry>
